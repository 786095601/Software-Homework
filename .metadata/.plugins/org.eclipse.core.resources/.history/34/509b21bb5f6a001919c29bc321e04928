class Work //å¼€åˆ›å·¥ä½œåºåˆ—
{
	static int X=10;
	static int Y=10;
	//è¿™ä¸ªæ£‹ç›˜å¤§å°çš„æ¥å£ä»¥åå†æ”¹ï¼Œç›®å‰å…ˆè¿™æ ·æ”¾ç€å§
	public String[][]maps;
	public void gen() //åˆå§‹åŒ–æ£‹ç›˜ç”Ÿæˆæ–¹æ³•
	{
      String [][]a=new String[X+1][Y+1];                    //å¼€X+1,Y+1ä¸ªä½ç½®æŠŠä¸€è¡Œä¸€åˆ—ç©ºå‡ºæ¥æ–¹ä¾¿è°ƒæ•´
      for(int i=1;i<=X;i++) 
      {
    	  for(int j=1;j<=Y;j++)
    	  {
    		  a[i][j]="ğŸˆ³ ";
    	  }
      }
     maps = a; //æŠŠè¿™ä¸ªåˆå§‹åŒ–çš„æ£‹ç›˜æ‹¿å‡ºæ¥
	}
	public void print() //åˆå§‹åŒ–æ‰“å°æ–¹æ³•
	{
		for(int i=1;i<=X;i++) 
	    {
	  	  for(int j=1;j<=Y;j++)
	  	  {
	  		System.out.print(maps[i][j]);
	  	  }
	  	    System.out.println();
	}
	}
